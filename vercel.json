{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.11" }
    },
    {
      "src": "api/process.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.11" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/main.py" }
  ]
}

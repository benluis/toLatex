{
  "functions": {
    "app/main.py": {
      "runtime": "@vercel/python"
    },
    "api/process.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/api/process", "dest": "/api/process.py" },
    { "src": "/(.*)", "dest": "/app/main.py" }
  ]
}
